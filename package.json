{
  "name": "tg-tree",
  "description": "A mobile tree component by Vue",
  "version": "0.0.1",
  "author": "金智教育 wisedu",
  "homepage": "http://github.com/wisedu",
  "packageName": "tg-tree",
  "main": "dist/tg-tree.min.js",
  "scripts": {
    "dev": "npm run buildcss && cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --config build/webpack.dev.config.js",
    "build": "node ./build/build.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.config.js",
    "deploy": "rm -rf dist && node ./build/deploy.js",
    "buildcss": "node ./build/buildcss.js"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-runtime": "^6.23.0",
    "core-js": "3.0.0-beta.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "moment": "^2.19.1",
    "my-local-ip": "^1.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "vue": "^2.5.2",
    "vue-router": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^0.9.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "element-theme-default": "^1.4.7",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-pxtorem": "^3.3.1",
    "shelljs": "^0.7.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.5.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  }
}
