{
  "name": "tg-tree",
  "description": "A mobile tree component by Vue",
  "version": "1.0.0",
  "author": "金智教育 wisedu",
  "homepage": "http://github.com/wisedu",
  "packageName": "tg-tree",
  "main": "dist/tg-tree.min.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config build/webpack.dev.config.js",
    "build": "node ./build/build.js && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.config.js",
    "deploy": "rm -rf dist && node ./build/deploy.js",
    "buildcss": "node ./build/buildcss.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "babel-runtime": "^6.23.0",
    "core-js": "3.0.0-beta.3",
    "es6-promise": "^4.2.8",
    "moment": "^2.19.1",
    "my-local-ip": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^0.9.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "shelljs": "^0.7.6",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.2",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.0"
  }
}
